<?xml version="1.0" encoding="utf-8"?>
<resources>

  <style name="HNews.AppBar.NewsContainer" parent="None">
    <item name="android:elevation">@dimen/toolbar_elevation</item>
    <item name="appBarShadowHeight">@dimen/app_bar_shadow_no_height</item>
    <item name="appBarLayout">@layout/include_app_bar</item>
  </style>

  <style name="HNews.FloatingActionButton" parent="None">
    <item name="android:background">@drawable/shape_fab_background</item>
    <item name="android:layout_gravity">bottom|right</item>
    <item name="android:layout_marginRight">@dimen/fab_margin</item>
    <item name="android:layout_marginBottom">@dimen/fab_margin</item>
    <item name="android:minWidth">@dimen/fab_min_size</item>
    <item name="android:minHeight">@dimen/fab_min_size</item>
    <item name="android:elevation">@dimen/fab_elevation</item>
  </style>


</resources>